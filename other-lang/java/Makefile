all: binarytrees fannkuchredux fasta mandelbrot nbody reversecomplement spectralnorm
clean: clean-binarytrees clean-fannkuchredux clean-fasta clean-mandelbrot clean-nbody clean-regexdna clean-reversecomplement clean-spectralnorm

binarytrees:
	javac binarytrees.java
clean-binarytrees:
	rm -f binarytrees*.class

fannkuchredux:
	javac fannkuchredux.java
clean-fannkuchredux:
	rm -f fannkuchredux*.class

fasta:
	javac fasta.java
clean-fasta:
	rm -f fasta*.class

mandelbrot:
	javac mandelbrot.java
clean-mandelbrot:
	rm -f mandelbrot*.class

nbody:
	javac nbody.java
clean-nbody:
	rm -f nbody*.class NBodySystem.class Body.class

regexdna:
	javac regexdna.java
clean-regexdna:
	rm -f regexdna*.class ByteWrapper.class

reversecomplement:
	javac reversecomplement.java
clean-reversecomplement:
	rm -f reversecomplement*.class

spectralnorm:
	javac spectralnorm.java
clean-spectralnorm:
	rm -f spectralnorm*.class
